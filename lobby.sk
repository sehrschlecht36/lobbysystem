#________________________________________________UPDATE_______________________________________________

on join:
	if text from "https://pastebin.com/raw/VdvSfnxX" is not "alpha-1.1":
		loop all players:
			if loop-player has permission "pm.update.notify":
				set {__newversion} to text from "https://pastebin.com/raw/KcNASsX1"
				set {__updates} to text from "https://pastebin.com/raw/1H7cKcPt"
				send "&e&m--------------------------------------------------" to player
				send "&a&lEine neue Version von LobbySystem ist verfügbar." to player
				send "https://forums.skunity.com/resources/lobbysystem-de.893/" to player
				send "&aNeuigkeiten in der Version &a&l%{__newversion}%:" to player
				send "%{__updates}%" to player
				send "&e&m--------------------------------------------------" to player
				
command /checkforupdate:
	permission: pm.update.check
	permission message: {@KeineRechte}
	trigger:
		if text from "https://pastebin.com/raw/VdvSfnxX" is not "alpha-1.1":
			loop all players:
				if loop-player has permission "pm.update.notify":
					set {__newversion} to text from "https://pastebin.com/raw/KcNASsX1"
					set {__updates} to text from "https://pastebin.com/raw/1H7cKcPt"
					send "&e&m--------------------------------------------------" to player
					send "&a&lEine neue Version von LobbySystem ist verfügbar." to player
					send "https://forums.skunity.com/resources/lobbysystem-de.893/" to player
					send "&aNeuigkeiten in der Version &a&l%{__newversion}%:" to player
					send "%{__updates}%" to player
					send "&e&m--------------------------------------------------" to player
#________________________________________________LOBBY-SETTINGS_______________________________________________
on join:
	if {prefix} is not set:
		loop all players:
			if loop-player has permission "pm.update.notify":
				send "§7[§6%{prefix}%§7]§a§l &4&lAchtung: Es ist kein Prefix gesetzt!" to player
command /color [<Text>]:
	permission: pm.lobby.color
	permission message: {@KeineRechte}
	trigger:
		if arg 1 is not set:
			send "§7[§6%{prefix}%§7]§6 Du kannst folgende Farben auswählen:" to player
			send "§7[§6%{prefix}%§7]§6 &1Blau&r, &aGrün&r, &4Rot&r, Reset." to player
		if arg 1 is set:
			if arg 1 is "Blau":
				if player has permission "pm.lobby.color.blue":
					set {%player%.color} to "blue"
					send "§7[§6%{prefix}%§7]§6 Deine Farbe wurde erfolreich auf &1Blau &6gesetzt." to player
				else:
					send "{@KeineRechte}" to player
			else:
				if arg 1 is "Grün":
					if player has permission "pm.lobby.color.green":
						set {%player%.color} to "green"
						send "§7[§6%{prefix}%§7]§6 Deine Farbe wurde erfolreich auf &aGrün &6gesetzt." to player
					else:
						send "{@KeineRechte}" to player
				else:
					if arg 1 is "Rot":
						if player has permission "pm.lobby.color.red":
							set {%player%.color} to "red"
							send "§7[§6%{prefix}%§7]§6 Deine Farbe wurde erfolreich auf &4Rot &6gesetzt." to player
						else:
							send "{@KeineRechte}" to player
					else:
						if arg 1 is "Reset":
							delete {%player%.color}
							send "§7[§6%{prefix}%§7]§6 Deine Farbe wurde erfolreich zurückgesetzt." to player
						else:
							send "§7[§6%{prefix}%§7]§6 Diese Farbe existiert nicht." to player
						
on command "help":
	if player don't have permission "pm.command.papercommand":
		send "{@KeineRechte}" to player
		cancel event
		
on command "pl":
	if player don't have permission "pm.command.papercommand":
		send "{@KeineRechte}" to player
		cancel event
		
on command "plugins":
	if player don't have permission "pm.command.papercommand":
		send "{@KeineRechte}" to player
		cancel event
		
on command "ver":
	if player don't have permission "pm.command.papercommand":
		send "{@KeineRechte}" to player
		cancel event

on command "version":
	if player don't have permission "pm.command.papercommand":
		send "{@KeineRechte}" to player
		cancel event
on command "?":
	if player don't have permission "pm.command.papercommand":
		send "{@KeineRechte}" to player
		cancel event

on chat:
	if {%player%.reply} is not set:
		if {%player%.lastmessage} contains chat message:
			cancel event
			send "§7[§6%{prefix}%§7]§6 Bitte spamme nicht." to player
		else:
			set {%player%.lastmessage} to chat message
			if player has permission "pm.lobby.chat":
				cancel event
				if {%player%.color} is set:
					if {%player%.color} is "blue":
						broadcast "&a%player% &7>> &r&1%chat-message%"
					if {%player%.color} is "red":
						broadcast "&a%player% &7>> &r&4%chat-message%"
					if {%player%.color} is "green":
						broadcast "&a%player% &7>> &r&a%chat-message%"
				else:
					broadcast "&a%player% &7>> &r%chat-message%"
			else:
				cancel event
				send "§7[§6%{prefix}%§7]§6 Du kannst in der Lobby nicht schreiben." to player
#________________________________________________SPAWN_______________________________________________
command /setspawn:
	permission: pm.spawn.set
	permission message: {@KeineRechte}
	trigger:
		set {spawn} to the player's location
		send "§7[§6%{prefix}%§7]§6 Der Spawn wurde erfolreich auf &a%{spawn}% &6gesetzt." to player
		
command /spawn [<Player>]:
	permission: pm.spawn.tp
	permission message: {@KeineRechte}
	trigger:
		if arg 1 is not set:
			teleport player to {spawn}
			send "§7[§6%{prefix}%§7]§6 Du wurdest erfolreich zum Spawn teleportiert." to player
		else:
			if player has permission "pm.spawn.tp.others":
				teleport arg 1 to {spawn}
				send "§7[§6%{prefix}%§7]§6 &a%arg 1% &6wurde erfolreich zum Spawn teleportiert." to player
				send "§7[§6%{prefix}%§7]§6 Du wurdest erfolreich zum Spawn teleportiert." to arg 1
				
on join:
	teleport player to {spawn}
	
#________________________________________________GUI_______________________________________________
command /getgui:
	permission: pm.gui.teleporter.get
	permission message: {@KeineRechte}
	trigger:
		add 1 compass named "&aTeleporter" to player's inventory

on join:
	if player has permission "pm.gui.teleporter.get":
		if player's inventory contains 1 compass:
			remove all compass from the player's inventory
			add 1 compass named "&aTeleporter" to player's inventory
 
on inventory click:
	if name of event-inventory = "&aTeleporter":
		event-slot = barrier
		index of event-slot = 8
		cancel event
		event-click type = left mouse button
		if player has permission "pm.gui.teleporter.leave":
			kick player due to "&aDu hast den Server verlassen."
		else:
			send "&cDu hast leider keine Berechtigung, diese Funktion des Teleporters zu nutzen." to player
	if name of event-inventory = "&aTeleporter":
		event-slot = diamond sword
		index of event-slot = 6
		cancel event
		event-click type = left mouse button
		if player has permission "pm.gui.teleporter.pvp":
			teleport player to {Warp.PvP}
		else:
			send "&cDu hast leider keine Berechtigung, diese Funktion des Teleporters zu nutzen." to player
	if name of event-inventory = "&aTeleporter":
		event-slot = red bed
		index of event-slot = 4
		cancel event
		event-click type = left mouse button
		if player has permission "pm.gui.teleporter.bedwars":
			teleport player to {Warp.Bedwars}
		else:
			send "&cDu hast leider keine Berechtigung, diese Funktion des Teleporters zu nutzen." to player
	if name of event-inventory = "&aTeleporter":
		event-slot = grass block
		index of event-slot = 2
		cancel event
		event-click type = left mouse button
		if player has permission "pm.gui.teleporter.skywars":
			teleport player to {Warp.Skywars}
		else:
			send "&cDu hast leider keine Berechtigung, diese Funktion des Teleporters zu nutzen." to player
on right click:
	if player has permission "pm.gui.teleporter.use":
		if item type is compass:
			if name of item is "&aTeleporter":
				open chest inventory with 1 rows named "&aTeleporter" to player
				set slot 4 of player's current inventory to red bed named "&aBedwars"
				set slot 2 of player's current inventory to grass block named "&aSkywars"
				set slot 6 of player's current inventory to diamond sword named "&aPvP"
				set slot 8 of player's current inventory to barrier named "&4Server verlassen"
	else:
		send "&cDu hast leider keine Berechtigung, den Teleporter zu nutzen" to player
		
#________________________________________________ANTICHEAT_______________________________________________
command /ac [<Text>]:
	permission: pm.ac.cmd
	permission message: {@KeineRechte}
	trigger:
		if arg 1 is not set:
			send "§7[§6%{prefix}%§7]§6 Du kannst zwischen an und aus wählen."
			send "§7[§6%{prefix}%§7]§6 Die aktuelle Einstellung ist &a%{ac}%" to player
			stop trigger
		else:
			if arg 1 is "an":
				set {ac} to "true"
				send "§7[§6%{prefix}%§7]§6 Das AntiCheat wurde erfolgreich aktiviert." to player
			else:
				if arg 1 is "aus":
					set {ac} to "false"
					send "§7[§6%{prefix}%§7]§6 Das AntiCheat wurde erfolreich deaktiviert." to player
				else:
					send "§7[§6%{prefix}%§7]§6 Bitte schreibe an oder aus." to player
on leftclick:
	if {ac} is "true":
		add 1 to {click.%player%}
		if player has permission "pm.ac.trust":
		else:
			if {click.%player%} >= 17:
				kick the player due to "&4ANTICHEAT&7 >> &cAutoclick"
				add 1 to {%player%.ac.autoclick}
				if {%player%.ac.autoclick} is more than 5:
					ban player due to "&4ANTICHEAT&7 >> &cAutoclick"
					clear {%player%.ac.autoclick}
every 1 seconds:
	loop all players:
		clear {click.%loop-player%}

every 60 seconds:
	if {ac} is "true":
		loop all players:
			clear {%loop-player%.ac.autoarmor}
every 1 seconds:
	if {ac} is "true":
		loop all players:
			if loop-player is in world "World":
				add 1 diamond chestplate named "§4§lAC" to slot a random integer between 10 and 26 of the loop-player's inventory
				wait 0.1 seconds
				remove all diamond chestplate named "§4§lAC" from the loop-player's inventory
	
on inventory click:
	if {ac} is "true":
		if event-item is diamond chestplate:
			if name of event-item is "§4§lAC":
				cancel event
				add 1 to {%player%.ac.autoarmor}
				if {%player%.ac.autoarmor} is more than 10:
					kick player due to "&4ANTICHEAT&7 >> &cAutoclick"
					ban player due to "&4ANTICHEAT&7 >> &cAutoclick"
					clear {%player%.ac.autoarmor}

command /clearaa:
	permission: pm.ac.aa.clear
	trigger:
		clear {%player%.ac.autoarmor}
		stop trigger

#________________________________________________SCOREBOARD_______________________________________________
every 2 seconds:
	loop all players:
		wipe loop-player's sidebar
		set name of sidebar of loop-player to "§7[§6%{prefix}%§7]§6"
		set score "&a" in sidebar of loop-player to 10
		set score "&6Server: &aLobby" in sidebar of loop-player to 9
		set score "&1" in sidebar of loop-player to 8
		set score "&6Shop: &ashop.deinserver.de" in sidebar of loop-player to 7
		set score "&7" in sidebar of loop-player to 6
		set score "&6Forum: &aforum.deinserver.de" in sidebar of loop-player to 5
		set score "&a" in sidebar of loop-player to 4
		set score "&6TeamSpeak³: &ats.deinserver.de" in sidebar of loop-player to 3
		set score "&2" in sidebar of loop-player to 2
		set score "&6Dein Kontostand: &a%{money.%loop-player's uuid%}%&a$" in sidebar of loop-player to 1
		set score "&r" in sidebar of loop-player to 10
		if ping of loop-player is 0:
			set score "&6Dein Ping: &aNicht verfügbar" in sidebar of loop-player to 11
		else:
			set score "&6Dein Ping: &a%ping of loop-player%" in sidebar of loop-player to 11
		
#________________________________________________REPORT_______________________________________________
command /report [<Player>] [<Text>]:
	permission: pm.report.report
	permission message: {@KeineRechte}
	trigger:
		send "§7[§6%{prefix}%§7]§6 Bitte warten..." to player
		loop 100 times:
			if {%arg 1%.report.%loop-number%} is not set:
				set {%arg 1%.report.%loop-number%} to arg 2
				send "§7[§6%{prefix}%§7]§6 Dein Report wurde erfolgreich versandt. Wir halten dich auf dem Laufenden." to player
				set {%arg 1%.reportplayer.%loop-number%} to player
				stop trigger
				
command /lreport [<Player>]:
	permission: pm.report.list
	permission message: {@KeineRechte}
	trigger:
		send "§7[§6%{prefix}%§7]§6 --Beginn der Liste--" to player
		loop 100 times:
			if {%arg 1%.report.%loop-number%} is not set:
				send "§7[§6%{prefix}%§7]§6 --Ende der Liste--" to player
				stop trigger
			else:
				send "§7[§6%{prefix}%§7]§6 Spieler: &a%arg 1% &7| &6Nummer: &a%loop-number% &7| &6Grund: &a%{%arg 1%.report.%loop-number%}%" to player
			
command /creport [<Player>]:
	permission: pm.report.clear
	permission message: {@KeineRechte}
	trigger:
		if arg 1 is set:
			send "§7[§6%{prefix}%§7]§6 --Beginn der Liste--" to player
			loop 100 times:
				if {%arg 1%.report.%loop-number%} is not set:
					send "§7[§6%{prefix}%§7]§6 --Fertig--" to player
					stop trigger
				else:
					delete {%arg 1%.report.%loop-number%}	
		else:
			send "§7[§6%{prefix}%§7]§6 Bitte gib einen Spieler an." to player
			
command /viewcase [<Player>] [<Integer>]:
	permission: pm.case.moderator
	permission message: {@KeineRechte}
	trigger:
		if arg 1 is not set:
			send "§7[§6%{prefix}%§7]§6 Bitte gib einen Spieler an." to player
		else:
			if arg 2 is not set:
				send "§7[§6%{prefix}%§7]§6 Bitte gib einen Fall an." to player
			else:
				if {%arg 1%.report.%arg 2%} is not set:
					send "§7[§6%{prefix}%§7]§6 Dieser Fall existiert nicht." to player
				else:
					send "§7[§6%{prefix}%§7]§6 Informationen zu Fall &a%arg 2%:" to player
					send "§7[§6%{prefix}%§7]§6 Grund der Meldung: &a%{%arg 1%.report.%arg 2%}%" to player
					send "§7[§6%{prefix}%§7]§6 Meldung kommt von: &a%{%arg 1%.reportplayer.%arg 2%}%" to player
					send "§7[§6%{prefix}%§7]§6 Dein Report wird bearbeitet." to {%arg 1%.reportplayer.%arg 2%}
					
					
command /editcase [<offline player>] [<Integer>] [<Text>] [<Text>]:
	permission: pm.case.moderator
	permission message: {@KeineRechte}
	trigger:
		if arg 1 is not set:
			send "§7[§6%{prefix}%§7]§6 Bitte gib einen Spieler an." to player
		else:
			if arg 2 is not set:
				send "§7[§6%{prefix}%§7]§6 Bitte gib einen Fall an." to player
			else:
				if {%arg 1%.report.%arg 2%} is set:
					if arg 3 is not set:
						send "§7[§6%{prefix}%§7]§6 Du kannst folgendes tun: ban, pardon, mute." to player
					else:
						if arg 3 is "ban":
							if arg 4 is not set:
								send "§7[§6%{prefix}%§7]§6 Du kannst den Spieler bannen. Bitte gib eine Grund an." to player
							else:
								execute command "/minecraft:ban %arg 1% %arg 4%"
								send "§7[§6%{prefix}%§7]§6 Der Spieler %arg 1% &6wurde erfolreich wegen %arg 4% &6gebannt." to player
								delete {%arg 1%.report.%arg 2%}
								delete {%arg 2%.reportplayer.%arg 2%}
						if arg 3 is "pardon":
							if arg 4 is not set:
								send "§7[§6%{prefix}%§7]§6 Du kannst den Spieler freischalten. Bitte gib einen Grund an." to player
							else:
								send "§7[§6%{prefix}%§7]§6 Der Spieler wird wegen %arg 4% freigeschaltet." to player
						if arg 3 is "mute":
							if arg 4 is not set:
								send "§7[§6%{prefix}%§7]§6 Du kannst den Spieler stummschalten. Bitte gib einen Grund an." to player
							else:
								execute command "/mute %arg 1% 100000"
								send "§7[§6%{prefix}%§7]§6 Der Spieler %arg 1% wurde erfolreich wegen %arg 4% stumm gestellt." to player
								
#________________________________________________WARP_______________________________________________
command /setwarp [<Text>] [<Text>]:
	permission: pm.setwarp
	permission message: {@KeineRechte}
	trigger:
		if arg 1 is set:
			if arg 2 is set:
				set {Warp.%arg 1%.permission} to arg 2
				set {Warp.%arg 1%} to player's location
				send "§7[§6%{prefix}%§7]§6 Warp gesetzt!" to player
				send "§7[§6%{prefix}%§7]§6 Permission gesetzt!" to player
				stop trigger
			else:
				send "§7[§6%{prefix}%§7]§6 Warp gesetzt!" to player
				set {Warp.%arg 1%} to player's location
				add arg 1 to {Warps::*}
				stop trigger
		else:
			send "{@Prefix} Du musst einen Namen wählen!" to player
			stop trigger
			
command /warp [<Text>]:
	permission: pm.warp
	trigger:
		if arg 1 is set:
			if {Warp.%arg 1%} is set:
				if {Warp.%arg 1%.permission} is set:
					if player has permission "%{Warp.%arg 1%.permission}%":
						teleport player to {Warp.%arg 1%}
						send "{@Prefix} Teleportiert...!" to player
						stop trigger
					else:
						send "{@KeineRechte}" to player
						stop trigger
				else:
					teleport player to {Warp.%arg 1%}
					send "{@Prefix} Teleportiert...!" to player
					stop trigger
			else:
				send "{@Prefix} Warp existiert nicht!" to player
				stop trigger
		else:	
			send "{@Prefix} Warps: &a%{Warps::*}%" to player
			stop trigger
			
command /delwarp [<Text>]:
	permission: pm.delwarp
	permission message: {@KeineRechte}
	trigger:
		if arg 1 is set:
			if {Warp.%arg 1%} is set:
				delete {Warp.%arg 1%.permission}
				if {Warp.%arg 1%.permission} is set:
				delete {Warp.%arg 1%}
				remove arg 1 from {Warps::*}
				send "{@Prefix} Warp gelöscht!" to player
				stop trigger
			else:
				send "{@Prefix} Dieser Warp existiert nicht!" to player
				stop trigger 
		else:
			send "{@Prefix} Du musst einen Namen wählen!" to player
			stop trigger
			
#________________________________________________STATS_______________________________________________
on join:
	if {kills.%player%} is not set:
		set {kills.%player%} to 0
	if {tode.%player%} is not set:
		set {tode.%player%} to 0


command /stats:
	permission: pm.stats.view
	permission message: {@KeineRechte}
	trigger:
		send "{@Prefix} Kills: %{kills.%player%}%" to player
		send "{@Prefix} Tode: %{tode.%player%}%" to player
		send "{@Prefix} K/D: %({kills.%player%}/{tode.%player%})%" to player
		stop trigger
		
on death:
	add 1 to {tode.%player%}
	add 1 to {kills.%attacker%}

command /resetstats [<Text>]:
	aliases: /rstats, /statsreset
	trigger:
		if arg 1 is not set:
			if player has permission "pm.stats.reset.own":
				send "{@Prefix} Deine Stats werden zurückgesetzt, wenn du innerhalb der nächsten 60 Sekunden /resetstats confirm eingibts." to player
				set {sreset.%player%} to true
				wait 60 seconds
				delete {sreset.%player%}
			else:
				send "{@KeineRechte}" to player
		if arg 1 is set:
			if arg 1 is "confirm":
				if {sreset.%player%} is set:
					if {sreset.%player%} is true:
						if player has permission "pm.stats.reset.own":
							delete {kills.%player%}
							delete {tode.%player%}
							delete {sreset.%player%}
							send "{@Prefix} Deine Stats wurden erfolgreich zurückgesetzt." to player
				else:
					send "{@Prefix} Du kannst diesen Befehl nicht nutzen, da es nichts zu bestätigen gibt." to player
			if arg 1 is not "confirm":
				if player has permission "pm.stats.reset.other":
					delete {kills.%arg 1%}
					delete {tode.%arg 1%}
					send "{@Prefix} Die Stats von &a%arg 1% &6wurden erfolgreich zurückgesetzt." to player 

#________________________________________________SPEED_______________________________________________
command /walkspeed [<Integer>]:
	permission: pm.speed.walk
	permission message: {@KeineRechte}
	trigger:
		if arg 1 is set:
			if arg 1 is 1:
				set the player's walk speed to 0.2
				send "{@Prefix} Deine Geschwindigkeit wurde auf &a%arg 1% &6gesetzt." to player
				stop trigger
			if arg 1 is 2:
				set the player's walk speed to 0.3
				send "{@Prefix} Deine Geschwindigkeit wurde auf &a%arg 1% &6gesetzt." to player
				stop trigger
			if arg 1 is 3:
				set the player's walk speed to 0.4
				send "{@Prefix} Deine Geschwindigkeit wurde auf &a%arg 1% &6gesetzt." to player
				stop trigger
			if arg 1 is 4:
				set the player's walk speed to 0.5
				send "{@Prefix} Deine Geschwindigkeit wurde auf &a%arg 1% &6gesetzt." to player
				stop trigger
			if arg 1 is 5:
				set the player's walk speed to 0.6
				send "{@Prefix} Deine Geschwindigkeit wurde auf &a%arg 1% &6gesetzt." to player
				stop trigger
			if arg 1 is 6:
				set the player's walk speed to 0.7
				send "{@Prefix} Deine Geschwindigkeit wurde auf &a%arg 1% &6gesetzt." to player
				stop trigger
			if arg 1 is 7:
				set the player's walk speed to 0.8
				send "{@Prefix} Deine Geschwindigkeit wurde auf &a%arg 1% &6gesetzt." to player
				stop trigger
			if arg 1 is 8:
				set the player's walk speed to 0.9
				send "{@Prefix} Deine Geschwindigkeit wurde auf &a%arg 1% &6gesetzt." to player
				stop trigger
			if arg 1 is 9:
				set the player's walk speed to 1
				send "{@Prefix} Deine Geschwindigkeit wurde auf &a%arg 1% &6gesetzt." to player
				stop trigger
			if arg 1 is 10:
				set the player's walk speed to 1.1
				send "{@Prefix} Deine Geschwindigkeit wurde auf &a%arg 1% &6gesetzt." to player
				stop trigger
		else:
			send "{@Prefix} Du kannst zwischen den Geschwindigkeiten 1-10 wählen." to player
			stop trigger
		stop trigger
			
command /resetspeed:
	permission: pm.speed.resetspeed
	permission message: {@KeineRechte}
	trigger:
		set the player's walk speed to 0.2
		set the player's fly speed to 0.1
		send "{@Prefix} Deine Geschwindigkeit wurde zurückgesetzt." to player
		
command /flyspeed [<Integer>]:
	permission: pm.speed.fly
	permission message: {@KeineRechte}
	trigger:
		if arg 1 is set:
			if arg 1 is 1:

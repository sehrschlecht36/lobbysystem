				if arg 3 is not set:
					add arg 2 to {money.%player's uuid%}
					send "§7[§6%{prefix}%§7]§6 &a%arg 2%&a$ &6wurden hinzugefügt!" to player
					stop trigger
				else:
					add arg 2 to {money.%arg 3's uuid%}
					send "§7[§6%{prefix}%§7]§6 &a%arg 2%&a$ &6wurden &a%arg 3% &6hinzugefügt!" to player
					stop trigger
			else:
				send "{@KeineRechte}" to player
		if arg 1 is "pay":
			send "§7[§6%{prefix}%§7]§6 Bitte benutze &a/pay&6, um zu bezahlen." to player
			stop trigger
		if arg 1 is "set":
			if player has permission "pm.eco.set":
				if arg 2 is not set:
					send "§7[§6%{prefix}%§7]§6 Bitte gib eine Menge an." to player
				else:
					if arg 3 is not set:
						set {money.%player's uuid%} to arg 2
						send "§7[§6%{prefix}%§7]§6 Dein Kontostand wurde erfolreich auf &a%arg 2%&a$ &6gesetzt." to player
					else:
						if player has permission "pm.eco.set.others":
							set {money.%arg 3's uuid%} to arg 2
							send "§7[§6%{prefix}%§7]§6 Der Kontostand von &a%arg 3% &6wurde erfolreich auf &a%arg 2% &6gesetzt." to player
						else:
							send "{@KeineRechte}" to player
			else:
				send "{@KeineRechte}" to player
		if arg 1 is "remove":
			if player has permission "pm.eco.remove":
				if arg 3 is not set:
					if player has permission "pm.eco.remove.own":
						remove arg 2 from {money.%player's uuid%}
						send "§7[§6%{prefix}%§7]§6 Dir wurden &a%arg 2%&a$ &6abgezogen." to player
						stop trigger
					else:
						send "{@KeineRechte}" to player
						stop trigger
				if arg 3 is set:
					if player has permission "pm.eco.remove.other":
						remove arg 2 from {money.%arg 3's uuid%}
						send "§7[§6%{prefix}%§7]§6 Du hast &a%arg 3% &a%arg 2%&a$ &6abgezogen." to player
						if arg 3 has permission "pm.eco.remove.notify":
							send "§7[§6%{prefix}%§7]§6 Dir wurden &a%arg 2%&a$ &6von &a%player% &6abgezogen." to arg 3
							stop trigger
					else:
						send "{@KeineRechte}" to player
						stop trigger
			else:
				send "{@KeineRechte}" to player
				stop trigger
		
command /pay [<player>] [<Integer>]:
	aliases: /givemoney
	permission: pm.eco.pay
	permission message: {@KeineRechte}
	trigger:
		if arg 1 is set:
			if arg 2 is set:
				if arg 2 is more than 0:
					if arg 2 is not more than {money.%player's uuid%}:
						remove arg 2 from {money.%player's uuid%}
						add arg 3 to {money.%arg 2's uuid%}
						send "§7[§6%{prefix}%§7]§6 Du hast &a%arg 1% &a%arg 2%&a$ &6gegeben." to player
						send "§7[§6%{prefix}%§7]§6 Du hast &a%arg 2%&a$ &6von &a%player% &6erhalten." to arg 1
						stop trigger
					else:
						send "§7[§6%{prefix}%§7]§6 Du hast nicht genug Geld dafür!" to player
						stop trigger
			if arg 2 is not set:
				send "§7[§6%{prefix}%§7]§6 Bitte gib eine Geldmenge an!" to player
				stop trigger
		else:
			send "§7[§6%{prefix}%§7]§6 Bitte gib einen Spieler an!" to player
			stop trigger

#________________________________________________SETLIFE_______________________________________________

on first join:
    set max health of player to 10
 
on respawn: 
    set max health of player to {Setlife.%player%}
 
 
command /setlife [<number>]:
    permission: pm.setlife
    trigger:
        if arg 1 is set:
            if arg 1 is more than 10:
                set max health of player to arg 1
                set {Setlife.%player%} to arg 1
            else:
                set max health of player to 10
                set {Setlife.%player%} to 10
            set Health of player to arg 1
            send "§7[§6%{prefix}%§7]§6 Deine Leben wurden auf &a%arg-1% &6gesetzt." to player
        else:
            send "§7[§6%{prefix}%§7]§6 Bitte gib eine Nummer ein!" to player

#________________________________________________BANSYSTEM_______________________________________________

command /ban [<offline player>] [<Text>]:
	permission: pm.ban.ban
	permission message: {@KeineRechte}
	trigger:
		if arg 1 is not set:
			send "§7[§6%{prefix}%§7]§6 Bitte gib einen Spieler an." to player
		else:
			if arg 2 is not set:
				set {%arg 1%.banned} to "§7[§6%{prefix}%§7]§6 Du bist gebannt."
				send "§7[§6%{prefix}%§7]§6 Der Spieler %arg 1% wurde erfolreich gebannt." to player
				kick arg 1 due to "§7[§6%{prefix}%§7]§6 Du wurdest gebannt."
			if arg 2 is set:
				set {%arg 1%.banned} to "§7[§6%{prefix}%§7]§6 Du bist wegen %arg 2% gebannt."
				send "§7[§6%{prefix}%§7]§6 Der Spieler %arg 1% wurde erfolgreich wegen %arg 2% gebannt." to player
				kick arg 1 due to "§7[§6%{prefix}%§7]§6 Du wurdest gebannt."
				
on join:
	if {%player%.banned} is set:
		kick player due to {%player%.banned}
		
command /unban [<offline player>]:
	permission: pm.ban.unban
	permission message: {@KeineRechte}
	trigger:
		if arg 1 is not set:
			send "§7[§6%{prefix}%§7]§6 Bitte gib einen Spieler an." to player
		else:
			delete {%arg 1%.banned}
			send "§7[§6%{prefix}%§7]§6 Der Spieler &a%arg 1% &6wurde erfolreich entbannt." to player

#________________________________________________MSG_______________________________________________

command /msg [<Player>] [<Text>]:
	aliases: /tell, /say, /whisper, /message, /m
	permission: pm.msg.msg
	permission message: {@KeineRechte}
	trigger:
		if arg 1 is not set:
			send "§7[§6%{prefix}%§7]§6 Bitte gib einen Spieler an." to player
		else:
			if arg 2 is not set:
				send "§7[§6%{prefix}%§7]§6 Bitte gib eine Nachricht an." to player
			else:
				send "§7[§6%{prefix}%§7]§6 &aDu &7-> &a%arg 1% &7| &r%arg 2%" to player
				send "§7[§6%{prefix}%§7]§6 &a%player% &7-> &adir &7| &r%arg 2%" to arg 1
				set {%player%.msg} to arg 1
				set {%arg 1%.msg} to player
				
on chat:
	if {%player%.reply} is true:
		cancel event
		send chat message to {%player%.msg}
		send "§7[§6%{prefix}%§7]§6 Deine Nachricht wurde erfolreich versandt." to player
		send "§7[§6%{prefix}%§7]§6 &aDu &7-> &a%{%player%.msg}% &7| &r%chat message%" to player
		send "§7[§6%{prefix}%§7]§6 &a%player% &7-> &adir &7| &r%chat message%" to {%player%.msg}
		delete {%player%.reply}

command /r:
	aliases: /reply, /antwort, /replymsg
	permission: pm.msg.reply
	permission message: {@KeineRechte}
	trigger:
		send "§7[§6%{prefix}%§7]§6 Bitte gebe einen Text in den Chat ein, den du senden willst."
		set {%player%.reply} to true
		

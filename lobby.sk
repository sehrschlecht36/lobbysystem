#________________________________________________MUTE_______________________________________________
every 1 minute:
	add 1 to {time}
	wait 5 ticks
	loop all players:
		if {mute.%loop-player's uuid%} is less than or equal to {time}:
			delete {mute.%loop-player's uuid%}
			send "§7[§6%{prefix}%§7]§6 Mute aufgehoben!" to loop-player


command /mute [<Player>] [<Integer>]:
	permission: pm.mute.mute
	permission message: {@KeineRechte}
	trigger:
		if arg 1 is not set:
			send "§7[§6%{prefix}%§7]§6 /mute [Spieler] [Zeit]" to player
			stop trigger
		loop all players:
			if arg 1 is loop-player:
				if arg 2 is set:
					send "§7[§6%{prefix}%§7]§6 Du wurdest für %arg 2% Minuten stumm gestellt!" to loop-player
					send "§7[§6%{prefix}%§7]§6 Du hast &a%arg 1% &6für &a%arg 2% &6Minuten stumm gestellt!" to player
					set {mute.%loop-player's uuid%} to ({time}+arg 2)
					stop trigger
					
					
					
on join:
	if {mute.%player's uuid%} is set:
		if {mute.%player's uuid%} is less than or equal to {time}:
			delete {mute.%player's uuid%}
			send "§7[§6%{prefix}%§7]§6 Mute aufgehoben!" to player
			stop trigger
				
				
on chat:
	if {mute.%player's uuid%} is set:
		cancel event
		send "§7[§6%{prefix}%§7]§6 Du bist stumm gestellt!" to player
		
command /unmute [<Player>]:
	permission: pm.mute.unmute
	permission message: {@KeineRechte}
	trigger:
		if arg 1 is not set:
			send "§7[§6%{prefix}%§7]§6 Du musst einen Spieler angeben." to player
			stop trigger
		else:
			loop all players:
				if arg 1 is loop-player:
					if {mute.%loop-player's uuid%} is set:
						delete {mute.%loop-player's uuid%}
						send "§7[§6%{prefix}%§7]§6 &a%arg 1% &6wurde erfolreich entmuted." to player
						send "§7[§6%{prefix}%§7]§6 Du wurdest erfolreich von &a%player% &6entmuted." to loop-player
						stop trigger
					else:
						send "§7[§6%{prefix}%§7]§6 Dieser Spieler ist nicht gemuted!" to player
						stop trigger
						

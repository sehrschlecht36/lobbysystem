command /sethome [<Text>]:
	trigger:
		if arg 1 is set:
			set {home.%player%.%arg 1%} to player's location
			add arg 1 to {homes.%player%::*}
			send "§7[§6%{prefix}%§7]§6 Zuhause gesetzt!" to player
			stop trigger
		else:
			send "§7[§6%{prefix}%§7]§6 Du musst einen Namen angeben" to player
			stop trigger
		
command /home [<Text>]:
	trigger:
		if arg 1 is not set:
			send "§7[§6%{prefix}%§7]§6 %{homes.%player%::*}%" to player
			stop trigger
		else:
			if {home.%player%.%arg 1%} is set:
				teleport player to {home.%player%.%arg 1%}
				send "§7[§6%{prefix}%§7]§6 Zuhause angekommen!" to player
				stop trigger
			else:
				send "§7[§6%{prefix}%§7]§6 Dieser HomePunkt existiert nicht!" to player
				stop trigger
		
command /delhome [<Text>]:
	trigger:
		if {home.%player%.%arg 1%} is set:
			delete {home.%player%.%arg 1%}
			remove arg 1 from {home.%player%::*}
			send "§7[§6%{prefix}%§7]§6 Zuhause gelöscht!" to player
			stop trigger
		else:
			send "§7[§6%{prefix}%§7]§6 Dieser HomePunkt existiert nicht!" to player
			stop trigger
			
